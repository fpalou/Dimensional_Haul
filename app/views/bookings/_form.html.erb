<%= simple_form_for [@dimension, booking] do |f| %>
<%= f.input :,
            as: :string,
            input_html: { data: { controller: "datepicker", range: "true" } } %>
<%= f.input :end_date,
            as: :hidden %>
<%= f.input :guests %>
<%= f.input :perks %>
<%= f.submit "Submit", class: "btn btn-success" %>
<% end %>
